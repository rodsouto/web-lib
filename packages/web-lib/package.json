{
	"name": "@yearn/web-lib",
	"version": "0.9.0",
	"main": "./dist/index.js",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"sideEffects": false,
	"type": "module",
	"repository": "git://github.com/yearn/web-lib.git",
	"publishConfig": {
		"registry": "https://npm.pkg.github.com",
		"access": "public"
	},
	"files": [
		"dist/**",
		"tailwind.config.cjs",
		"style.css",
		"build.css"
	],
	"exports": {
		".": "./dist/index.js",
		"./style.css": "./dist/style.css",
		"./build.css": "./dist/build.css",
		"./tailwind.plugin": "./dist/tailwind.config.cjs",
		"./layouts": "./dist/layouts/index.js",
		"./components": "./dist/components/index.js",
		"./contexts": "./dist/contexts/index.js",
		"./hooks": "./dist/hooks/index.js",
		"./icons": "./dist/icons/index.js",
		"./utils": "./dist/utils/index.js"
	},
	"scripts": {
		"dev": "tsc --jsx react --esModuleInterop -m esnext -p ./tsconfig.json --watch --outDir dist",
		"exportStyle": "npx tailwindcss -c ./config/tailwind.config.cjs -o dist/build.css --minify && sed -i -e 's/rem/em/g' dist/build.css",
		"build": "./scripts/build.sh --external:react --external:react-dom --platform=node",
		"prepare": "rm -rf dist && yarn build && yarn exportStyle",
		"publish-github": "sed -i -e 's|\"@yearn-finance/web-lib\"|\"@yearn/web-lib\"|' package.json && sed -i -e 's|\"https://registry.npmjs.org\"|\"https://npm.pkg.github.com\"|' package.json && yarn publish",
		"publish-npm": "sed -i -e 's|\"@yearn/web-lib\"|\"@yearn-finance/web-lib\"|' package.json && sed -i -e 's|\"https://npm.pkg.github.com\"|\"https://registry.npmjs.org\"|' package.json && yarn publish",
		"lint": "TIMING=1 eslint **/*.ts* --ignore-pattern 'node_modules/**' --ignore-pattern 'dist/'",
		"lint-fix": "TIMING=1 eslint **/*.ts* --ignore-pattern 'node_modules/**' --ignore-pattern 'dist/' --fix",
		"clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
	},
	"dependencies": {
		"@coinbase/wallet-sdk": "^3.1.0",
		"@ethersproject/experimental": "^5.6.2",
		"@headlessui/react": "^1.6.3",
		"@tailwindcss/forms": "^0.5.2",
		"@tailwindcss/line-clamp": "^0.4.0",
		"@tailwindcss/typography": "^0.5.2",
		"@walletconnect/ethereum-provider": "^1.7.8",
		"@walletconnect/jsonrpc-http-connection": "^1.0.0",
		"@walletconnect/jsonrpc-provider": "^1.0.3",
		"@web3-react/coinbase-wallet": "^8.0.22-beta.0",
		"@web3-react/core": "8.0.30-beta.0",
		"@web3-react/eip1193": "8.0.14-beta.0",
		"@web3-react/gnosis-safe": "^8.0.0-beta.0",
		"@web3-react/metamask": "8.0.16-beta.0",
		"@web3-react/types": "8.0.17-beta.0",
		"@web3-react/walletconnect": "8.0.23-beta.0",
		"dayjs": "^1.11.2",
		"ethcall": "^4.7.0",
		"ethers": "^5.6.8",
		"eventemitter3": "^4.0.7",
		"framer-motion": "^6.3.4",
		"nprogress": "^0.2.0",
		"react-flip-move": "^3.0.4",
		"react-hot-toast": "^2.2.0",
		"react-virtualized": "^9.22.3",
		"tailwindcss": "^3.1.0"
	},
	"peerDependencies": {
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"tailwindcss": "^3.0.24"
	},
	"devDependencies": {
		"@types/node": "^17.0.35",
		"@types/react": "^18.0.9",
		"@types/react-dom": "^18.0.5",
		"@typescript-eslint/eslint-plugin": "^5.26.0",
		"@typescript-eslint/parser": "^5.26.0",
		"eslint": "^8.16.0",
		"eslint-config-next": "^12.1.6",
		"eslint-import-resolver-typescript": "^2.7.1",
		"eslint-plugin-import": "^2.26.0",
		"eslint-plugin-react": "^7.30.0",
		"eslint-plugin-react-hooks": "^4.5.0",
		"eslint-plugin-tailwindcss": "^3.5.0",
		"postcss": "^8.4.14",
		"postcss-import": "^14.1.0",
		"postcss-nesting": "^10.1.7",
		"react": "^18.1.0",
		"tailwindcss": "^3.0.24",
		"ts-loader": "^9.3.0",
		"tsup": "^6.0.1",
		"typescript": "^4.7.2"
	}
}
